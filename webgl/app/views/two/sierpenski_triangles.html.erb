<body onload="webgl.two.sierpenski_triangles.init()">
  <script id="ui-fragment-shader" type="x-shader/x-fragment">
    void main(void) {
      gl_FragColor = vec4(1.0,1.0,1.0,1.0);
    }
  </script>
  <script id="ui-vertex-shader" type="x-shader/x-vertex">
    attribute vec3 aSierpenskiPosition;
  
    uniform mat4 uMVMatrix;
    uniform mat4 uPMatrix;
  
    void main(void) {
      gl_PointSize = 0.5;
      gl_Position = uPMatrix * uMVMatrix * vec4(aSierpenskiPosition, 1.0);
    }
  </script>
  <div class="panel-container">
    <div>
      <a href="/two" class="button back-link">..</a>
    </div>
    <br><br>
    <h1>Learnings (Two : Sierpenski Triangles)</h1>
    <br>
    <canvas id="ui-canvas"></canvas>
  </div>
</body>